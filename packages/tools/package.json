{
  "name": "tools",
  "version": "0.0.1",
  "description": "set of common tools for Cloudinary API",
  "keywords": [
    "cloudinary",
    "color",
    "rgb",
    "hex",
    "rgba",
    "color",
    "conversion",
    "js",
    "javascript"
  ],
  "author": "Maya Shavin <maya@cloudinary.com>",
  "homepage": "https://github.com/mayashavin/cloudinary-api/tree/master/packages/tools#readme",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mayashavin/cloudinary-api.git"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json && tsc -p tsconfig.json",
    "test:unit": "jest __tests__ --reporters default",
    "tsc": "tsc -p tsconfig.build.json && tsc -p tsconfig.json",
    "prepublishOnly": "agadoo dist/"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "agadoo": "^2.0.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-html-reporters": "^2.1.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "bugs": {
    "url": "https://github.com/mayashavin/cloudinary-api/issues"
  }
}
